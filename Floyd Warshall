class Solution {
  public:
    void floydWarshall(vector<vector<int>> &dist) {
        // Code here
        int m=dist.size();
        for(int via=0;via<m;via++){
            for(int i=0;i<m;i++){
             for(int j=0;j<m;j++){
                 if(dist[i][via]!=1e8 && dist[via][j]!=1e8){
                 dist[i][j]=min(dist[i][j],dist[i][via]+dist[via][j]);}
             }   
            }
        }
    }
};
