class Solution {
  public:
    string removeSpecialCharacter(string s) {
        // code here
        string res;
        string str="-1";
        int n=s.length();
        for(int i=0;i<n;i++){
            if(isalpha(s[i])){
               res+=s[i]; 
            }
        }
        if(res.empty())return str; 
        return res;
    }
};
