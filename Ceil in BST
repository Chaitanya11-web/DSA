class Solution {
  public:
    int findCeil(Node* root, int x) {
        // code here
   
        // code here
        if(root==NULL){
            return -1;
        }
        if(root->data==x){
            return x;
        }
        if(root->data<x){
            return findCeil(root->right,x);
        }
      int floorLeft=findCeil(root->left,x);
      if(floorLeft==-1){
          return root->data;
      }
      else{
          return floorLeft;
      }
      return -1;
    }
};
    
