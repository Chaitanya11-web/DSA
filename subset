class Solution {
public:
vector<vector<int>> sub(vector<int>& nums,vector<int>& curr,int index, vector<vector<int>>&ans){
    if(index==nums.size()){
        ans.push_back(curr);
        return ans;
    }
    curr.push_back(nums[index]);
    sub(nums,curr,index+1,ans); 
    curr.pop_back();
    sub(nums,curr,index+1,ans);
    return ans;
}
    vector<vector<int>> subsets(vector<int>& nums) {
        int n=nums.size();
    vector<vector<int>>ans;
    vector<int>curr;
     return sub(nums,curr,0,ans);  
    }
};
