#include <iostream>
#include<map>
#include<string>
using namespace std;
void calculate(string s){
    int n= s.length();
    map<char,int>mp;
    for(int i=0;i<n;i++){
        mp[s[i]++];
    }
    
    for(auto it:mp){
        cout<<it.first<<" :"<<it.second<<endl;
    }
}

int main() {
    // Your code goes here
    calculate("hello geeks");
    return 0;
}
