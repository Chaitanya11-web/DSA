//BRUTE FORCE
#include <iostream>
#include<map>
#include<string>
using namespace std;
void calculate(string s){
    int n= s.length();
    map<char,int>mp;
    for(int i=0;i<n;i++){
        mp[s[i]++];
    }
    
    for(auto it:mp){
        cout<<it.first<<" :"<<it.second<<endl;
    }
}

int main() {
    // Your code goes here
    calculate("hello geeks");
    return 0;
}
//time and space-O(n)

class Solution {
  public:
    string removeDuplicates(string &s) {
        // code here
        string res;
        unordered_set<int>st;
        for(char c:s){
           if(st.find(c)==st.end()){
               st.insert(c);
               res.push_back(c);
           } 
        }
        return res;
    }
};
