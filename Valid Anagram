class Solution {
public:
    bool isAnagram(string s, string t) {
        if(s.length()!=t.length())return false;
        bool ans=true;
        unordered_map<char,int>map1;
          unordered_map<char,int>map2;
          for(char c:s){
            map1[c]++;
          }
          for(char c:t){
            map2[c]++;
          }
          for(auto it:map1){
            char ch=it.first;
            int count=it.second;
            if((map2.find(it.first)==map2.end()) || (map2[ch]!=count)){
                ans=false;
                return ans;
            }
          }
          return ans;
    }
};
