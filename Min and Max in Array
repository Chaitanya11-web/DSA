class Solution {
  public:
    vector<int> getMinMax(vector<int> &arr) {
        // code here
        vector<int>temp;
        int max=arr[0];
        int min=arr[0];
        for(int i=0;i<arr.size();i++){
            if(arr[i]<min){
                min=arr[i];
            }
            else if(arr[i]>max){
                max=arr[i];
            }
        }
        temp.push_back(min);
        temp.push_back(max);
        return temp;
    }
};
