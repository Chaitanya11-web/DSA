/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode() : val(0), left(nullptr), right(nullptr) {}
 *     TreeNode(int x) : val(x), left(nullptr), right(nullptr) {}
 *     TreeNode(int x, TreeNode *left, TreeNode *right) : val(x), left(left), right(right) {}
 * };
 */
class Solution {
public:
bool Symmetric(TreeNode* Lroot,TreeNode* Rroot){
if(Lroot==NULL && Rroot==NULL)return 1;
if((Lroot && !Rroot)||(!Lroot && Rroot))return 0;
if(Lroot->val!=Rroot->val)return 0;
 return Symmetric(Lroot->left,Rroot->right)&& Symmetric(Lroot->right,Rroot->left);
 }
    bool isSymmetric(TreeNode* root) {
    TreeNode* Lroot=root->left;
     TreeNode* Rroot=root->right;

      return Symmetric(Lroot, Rroot);
    }
};
