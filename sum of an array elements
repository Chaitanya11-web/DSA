// User function template for C++
class Solution {
  public:
  int sum(vector<int>& arr,int n,int i){

        if(i==n)return 0;
        return arr[i]+sum(arr,n,i+1);   
  }
    // Function to return sum of elements
    int arraySum(vector<int>& arr) {
        // code here
            int n=arr.size();
     return sum(arr,n,0);
    }
};
