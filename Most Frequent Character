class Solution {
  public:
    char getMaxOccuringChar(string& s) {
        //  code here
        map<char,int>mp;
        int n=s.length();
        for(int i=0;i<n;i++){
            mp[s[i]]++;
        }
        int maxi=0;
        char res='\0';
        for(auto it:mp){
           if(it.second>maxi){
               maxi=it.second;
               res=it.first;
           }
        }
        return res;
    }
};
