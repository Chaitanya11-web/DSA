//brute force
class Solution {
  public:
    int median(vector<vector<int>> &mat) {
        // code here
         int r=mat.size();
        int c=mat[0].size();
        vector<int>res;
       
       for(int i=0;i<r;i++){
           for(int j=0;j<c;j++){
               res.push_back(mat[i][j]);
           }
       }
       sort(res.begin(),res.end());
       
       int n1=r*c;
       return res[n1/2];
    }
};
