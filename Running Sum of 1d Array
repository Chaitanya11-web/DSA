class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        vector<int>temp(nums.size());
       
        int n=nums.size();
        //add current and previous
      for(int i=1;i<n;i++){
        nums[i]=nums[i-1]+nums[i];
      } 
      for(int i=0;i<n;i++){
        temp[i]=nums[i];
      } 
   return temp; }
};
