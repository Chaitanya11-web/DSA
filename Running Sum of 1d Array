class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
      vector<int>temp;
      int sum=0;
      int n=nums.size();
     for(int i=0;i<n;i++){
        for(int j=0;j<=i;j++){
            sum+=nums[j];

        }
        temp.push_back(sum);
        sum=0;
     }
return temp;
    }
};
