class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int el=0;
        int count=0;
       for(int i=0;i<nums.size();i++){
if(count==0){
    el=nums[i];
    count=1;
}
else if(el==nums[i]){
    count++;
}
else{
    count--;
}
       } 
       count=0;
       for(int i=0;i<nums.size();i++){
        if(nums[i]==el){
            count++;
        }
        if(count>nums.size()/2)return el;
       }
       return 0;
    }
};
