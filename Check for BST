/*
class Node {
public:
    int data;
    Node* left;
    Node* right;

    Node(int val) {
        data = val;
        left = right = nullptr;
    }
};
*/

class Solution {
 bool inorder(Node* root,int& prev,bool& ans){
       if(root==NULL) {
           return NULL;
       }
       inorder(root->left,prev,ans);
       if(prev<root->data){
           prev=root->data;
           
       }
       else{
           ans=false;
           return ans;
       }
    inorder(root->right,prev,ans);
       return ans;
    }
  public:
    bool isBST(Node* root) {
        bool ans=true;
        // code here
        int prev=INT_MIN;
 inorder(root,prev,ans);
 return ans;
    }
};
