#include <bits/stdc++.h> 
void build(vector<int>& arr,int n,int val,int& index){
if(index==n){
	return;
}
arr[index]=val;
int current=index;
while(current>0 && arr[(current-1)/2]<arr[current]){
	swap(arr[(current-1)/2],arr[current]);
	current=(current-1)/2;
}
index++;
}
vector<int> buildHeap(vector<int> arr, int n)
{	int index=0;

	// Write your code here 
	for(int i=0;i<n;i++){
		build(arr,n,arr[i],index);
	}
	return arr;
}
