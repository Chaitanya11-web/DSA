class Solution {
public:
    bool isPalindrome(string s) {
     string res;
     int n=s.length();
     for(int i=0;i<n;i++){
        if(isalnum(s[i])){
            res+=tolower(s[i]);
        }
     } 
     int i=0;
     int j=res.length()-1;
     while(i<j){
        if(res[i]!=res[j]){
            return false;}
        i++;
        j--;
     }  
     return true;
    }
};
