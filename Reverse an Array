https://www.naukri.com/code360/problems/reverse-the-array_1262298void reverseArray(vector<int> &arr , int m) {
    // Write your code here   
    int n=arr.size();
    int i=m+1;
    int j= n-1;
    while(i<j){
        int temp=arr[i];
        arr[i]=arr[j];
        arr[j]=temp;
        i++;
        j--;
    }   	
}
